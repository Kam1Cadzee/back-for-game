{
  "name": "back-for-game",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "build": "cd client && npm run build",
    "start": "ts-node src/app",
    "watch": "nodemon src/app.ts",
    "server": "npm run migrate && npm run generate:prisma",
    "generate": "npm run genereate:prisma && npm run genereate:types",
    "genereate:prisma": "npx prisma generate",
    "genereate:types": "ts-node-dev --transpileOnly generatedTypes.ts",
    "migrate": "npm run migrate:save && npm run migrate:up",
    "migrate:save": "npx prisma2 migrate save --experimental",
    "migrate:up": "npx prisma2 migrate up --experimental"
  },
  "dependencies": {
    "@prisma/cli": "2.0.0-beta.2",
    "@prisma/client": "2.0.0-beta.2",
    "@types/express": "^4.17.3",
    "@types/fs-extra": "^8.1.0",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.3.8",
    "apollo-server-express": "^2.11.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.0",
    "graphql": "^14.1.1",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.2.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.8.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "type-graphql": "^0.17.6",
    "typegraphql-prisma": "^0.2.1",
    "typescript": "^3.8.3"
  }
}
